{
  "name": "RuBored",
  "manifest_version": 2,
  "version": "1.0",
  "description": "A place to let out your boredom",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["contextMenus", "tabs"],
  "web_accessible_resources": ["*.html"]
}
